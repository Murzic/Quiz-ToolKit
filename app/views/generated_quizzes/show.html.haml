.panel.panel-default.col-md-offset-3.col-md-6
  %ul.nav.nav-tabs    
    %li= link_to "Courses", user_courses_path(current_user)
    %li= link_to "Student Groups", student_groups_path
    %li.active= link_to "Generated Quizzes"
  %ol.breadcrumb
    %li= link_to "Generated Quizzes", generated_quizzes_path
    %li.active= @quiz.name
  .col-md-12
    .pull-right
      =link_to "Upload images", "", class: "btn btn-primary"

  .row
  .panel-body

    .panel.panel-default
      .panel-heading Attributes:
      .list-group
        - @generated_quiz.attributes.each do |key, value|
          .list-group-item
            .row
              .col-md-4= key
              .col-md-8= value

